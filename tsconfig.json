{
    "compilerOptions": {
        "allowJs": false,
        "target": "es6",
        "noImplicitReturns":true,
        "noEmitOnError":true,
        "moduleResolution":"node",
        "module":"commonjs",
        "sourceMap": true,
        "strict":true,
        "strictPropertyInitialization":false,
        "noImplicitReturns":false,
        "outDir":"compiled_node",
        "baseUrl":".",
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        }
    },
    "include": [
        "src/*"
    ]
}
